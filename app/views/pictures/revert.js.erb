var updatevalues;
updatevalues = function(c) {
  var par;
  par = "?x=" + c.x + "&y=" + c.y + "&w=" + c.w + "&h=" + c.h;
  $("#croppic").attr('data-params',par);
};

$("#pic_container_<%= @picture.id %>").html("<img id='target-<%= @picture.id%>'>");
$('#target-<%= @picture.id %>').attr('src','<%= @picture.file.url%>'+new Date())
$('#target-<%= @picture.id %>').Jcrop({
				      onChange:   updatevalues
				    });
$('#croppic').attr('data-original','true');
$('#retpicplace').html('<%= j render "ret" %>');
alert("<%= t('.source_restored')%>")